{
  "name": "react-extreme-admin",
  "version": "1.0.0",
  "description": "基于 react antd webpack 开发的一款后台管理系统模板",
  "scripts": {
    "start": "cross-env NODE_ENV=DEVELOPMENT node scripts/devServer.mjs",
    "build:production": "cross-env NODE_ENV=PRODUCTION  node scripts/build.mjs",
    "build:analyse": "cross-env NODE_ENV=ANALYSE  node scripts/build.mjs",
    "lint:eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,html,md}\"",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "commit": "git-cz"
  },
  "keywords": [
    "react",
    "redux",
    "antd",
    "webapck"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@ant-design/cssinjs": "^1.5.6",
    "@micro-zoe/micro-app": "^0.8.10",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-react": "^1.0.6",
    "ahooks": "^3.7.5",
    "antd": "^5.1.5",
    "axios": "^1.3.2",
    "draft-js": "^0.11.7",
    "echarts": "^5.4.1",
    "i18next": "^22.4.10",
    "immer": "^9.0.19",
    "js-cookie": "^3.0.1",
    "js-md5": "^0.7.3",
