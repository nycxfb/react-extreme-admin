    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-activation": "^0.12.2",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.2",
    "screenfull": "^6.0.2"
