    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "chalk": "^2.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.6",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "cssnano": "^5.1.15",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.37.0",
